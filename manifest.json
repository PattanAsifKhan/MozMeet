{
    "manifest_version": 2,
    "name": "Mozilla Meetups",
    "version": "1.0",

    "description": "Shows meetup events for Mozilla",

    "permissions":[
        "*://api.meetup.com/*"
    ],

    "browser_action":{
        "default_title":"Mozilla Meetups",
        "default_popup":"/popup/popup.html"
    }
}